    <constraintList numConstraints="4">
		<constraint>
            <file>Objective_Powell_3490</file>
            <comparisonOperator>IsValid</comparisonOperator>
            <scaleFactor>1000</scaleFactor>
            <aggregationMethod>SumOfSatisVals</aggregationMethod>
        </constraint>
		<constraint>
            <file>Objective_Lee_Ferry_Deficit</file>
            <comparisonOperator>IsValid</comparisonOperator>
            <scaleFactor>1000</scaleFactor>
            <aggregationMethod>SumOfSatisVals</aggregationMethod>
        </constraint>
		<constraint>
            <file>Objective_Mead_1000</file>
            <comparisonOperator>IsValid</comparisonOperator>
            <scaleFactor>1000</scaleFactor>
