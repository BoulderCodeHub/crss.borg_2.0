	<!-- <fileDrivenRun inputFile="current_policy_ForFileDrivenRun.txt"/> -->
		<populationSize>3</populationSize>
		<maxEvaluations>10</maxEvaluations>
    <!-- numVar equals sum of <column> <numRow> values -->
    <variableList numVar="10">
        <!-- either a single <variable> element which
             is applied to all variables or a <variable>
             element for each variable -->
        <variable>
			<!--RMS: letting all borg variables have 0-1 range for now but may need to change this--> 
            <bounds lower="0.0" upper="1.0"/>
        </variable>
    </variableList>
    <objectiveList numObj="4"> 
    	<objective>
            <file>Objective_Powell_3490</file>
            <epsilon>1</epsilon>
            <function>AVERAGE</function>
			<nanReplacement>99999</nanReplacement>
        </objective>
		<objective>
            <file>Objective_Lee_Ferry_Deficit</file>
            <epsilon>3</epsilon>
            <function>AVERAGE</function>
			<nanReplacement>99999</nanReplacement>
        </objective>
		<objective>
            <file>Objective_Mead_1000</file>
            <epsilon>1</epsilon>
            <function>AVERAGE</function>
			<nanReplacement>99999</nanReplacement>
        </objective>
		<objective>
            <file>Objective_LB_Shortage_Volume</file>
            <epsilon>50000</epsilon>
            <function>AVERAGE</function>
			<nanReplacement>99999</nanReplacement>
        </objective>
		</objectiveList>
		
	<metricList numMetrics="4"> 
    	<metric>
            <file>CRSP_Storage</file>
            <aggregationMethod>Average</aggregationMethod>
		</metric>
		<metric>
            <file>Sum_Powell_Releases</file>
            <aggregationMethod>Average</aggregationMethod>
